            <constraint field="selections" scope="roster" value="1.0" percentValue="false" shared="true" includeChildSelections="true" includeChildForces="false" id="f7e1-9554-2aa2-a614" type="max"/>
          </constraints>
          <profiles>
            <profile id="af7c-6e65-2424-a168" name="Headhunter" hidden="false" typeId="72c5eafc-75bf-4ed9-b425-78009f1efe82" typeName="Abilities">
              <characteristics>
                <characteristic name="Description" typeId="21befb24-fc85-4f52-a745-64b2e48f8228">This Warlord can target CHARACTER units even if they are not the closest enemy unit. When resolving an attack made with a ranged weapon by this Warlord, an unmodified hit roll of 6 inflicts 1 mortal wound on the target in addition to any other damage.</characteristic>
              </characteristics>
            </profile>
          </profiles>
          <costs>
            <cost name=" PL" typeId="e356-c769-5920-6e14" value="0.0"/>
